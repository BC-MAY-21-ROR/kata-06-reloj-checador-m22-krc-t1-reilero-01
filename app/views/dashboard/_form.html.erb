<%= form_with(url: employee_create_path) do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name, placeholder: "Petter Pipper" %>
  </div>

  <div>
    <%= f.label :email %>
    <%= f.email_field :email, placeholder: "petter@email.com" %>
  </div>

  <div>
    <%= f.label :position %>
    <%= f.text_field :position, placeholder: "Job Position" %>
  </div>

  <div>
    <%= f.label :private_number %>
    <%= f.text_field :private_number, placeholder: "#12" %>
  </div>

  <div>
    <%= f.label :store_id %>
    <%= f.text_field :store_id, placeholder: "Store #1" %>
  </div>

  <div>
    <%= f.submit "Create" %>
  </div>

<% end %>